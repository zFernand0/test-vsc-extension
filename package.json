{
	"name": "test-vsc-extension",
	"displayName": "Test VSCode Extension",
	"description": "Minimal VSCode Extension to help diagnose problems",
	"version": "0.0.1",
	"publisher": "zFernand0",
	"license": "MIT",
	"engines": {
		"vscode": "^1.65.0"
	},
	"contributes": {
		"commands": [
      {
        "command": "zFernand0.testCommand",
        "title": "Test Command",
        "category": "zFernando"
      }
		]
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "tsc -p ./ && npm run lint",
		"watch": "tsc -w -p ./",
		"pretest": "npm run build",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"eslint-plugin-license-header": "^0.4.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@zowe/core-for-zowe-sdk": "^7.0.0-next.202203231654",
		"@zowe/imperative": "file:../../zowe/imperative"
	}
}
